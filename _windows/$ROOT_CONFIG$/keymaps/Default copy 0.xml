<keymap version="1" name="Default copy 0" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <mouse-shortcut keystroke="button2 doubleClick" />
    <keyboard-shortcut first-keystroke="alt subtract" />
  </action>
  <action id="BuildArtifact">
    <keyboard-shortcut first-keystroke="alt b" />
  </action>
  <action id="EditorCompleteStatement">
    <keyboard-shortcut first-keystroke="enter" />
  </action>
  <action id="EditorEnter">
    <keyboard-shortcut first-keystroke="shift ctrl enter" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <mouse-shortcut keystroke="button3 doubleClick" />
    <keyboard-shortcut first-keystroke="alt add" />
  </action>
  <action id="QuickList.Building&amp;TabWindows">
    <keyboard-shortcut first-keystroke="alt quote" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt s" second-keystroke="v" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="shift ctrl f11" />
  </action>
</keymap>