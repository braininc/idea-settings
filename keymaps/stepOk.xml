<keymap version="1" name="stepOk" parent="$default">
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl space" />
    <keyboard-shortcut first-keystroke="alt space" />
  </action>
  <action id="EditorCompleteStatement">
    <keyboard-shortcut first-keystroke="enter" />
  </action>
  <action id="EditorEnter">
    <keyboard-shortcut first-keystroke="shift ctrl enter" />
  </action>
  <action id="NewScratchFile">
    <keyboard-shortcut first-keystroke="alt minus" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt numpad6" />
    <keyboard-shortcut first-keystroke="alt open_bracket" />
  </action>
  <action id="PinActiveTab">
    <keyboard-shortcut first-keystroke="alt back_slash" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt numpad4" />
    <keyboard-shortcut first-keystroke="alt close_bracket" />
  </action>
  <action id="QuickList.Building&amp;TabWindows">
    <keyboard-shortcut first-keystroke="alt semicolon" />
  </action>
  <action id="QuickList.Debugging&amp;VCS&amp;IDE">
    <keyboard-shortcut first-keystroke="alt quote" />
  </action>
  <action id="SmartTypeCompletion">
    <keyboard-shortcut first-keystroke="shift ctrl space" />
    <keyboard-shortcut first-keystroke="shift alt space" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>